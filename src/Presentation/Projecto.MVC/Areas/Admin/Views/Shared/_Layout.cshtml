@{
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Dashboard | Adminto - Responsive Bootstrap 4 Admin Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Sheikh Abdul Wahab Ali" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Admin/assets/images/favicon.ico">

    <!-- Bootstrap Css -->
    <link href="~/Admin/assets/css/bootstrap.min.css" id="bootstrap-stylesheet" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/Admin/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/Admin/assets/css/app.min.css" id="app-stylesheet" rel="stylesheet" type="text/css" />
    <!--Custom Css-->
    <link href="~/Admin/assets/css/CustomLayoutStyles.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.1/css/bootstrap-select.min.css">
    @await RenderSectionAsync("CreateTarotCardPageStyles", required: false)
    @await RenderSectionAsync("EditTarotCardPageStyles", required: false)
    @await RenderSectionAsync("ListUsersStyles", required: false)
   
</head>

<body data-layout="horizontal" data-topbar="dark">

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Navigation Bar-->
        <header id="topnav">

            <!-- Topbar Start -->
            <div class="navbar-custom">
                <div class="container-fluid">
                    <ul class="list-unstyled topnav-menu float-right mb-0">

                        <li class="dropdown notification-list">
                            <!-- Mobile menu toggle-->
                            <a class="navbar-toggle nav-link">
                                <div class="lines">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </div>
                            </a>
                            <!-- End mobile menu toggle-->
                        </li>

                        <li class="d-none d-sm-block">
                            <form class="app-search">
                                <div class="app-search-box">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search...">
                                        <div class="input-group-append">
                                            <button class="btn" type="submit">
                                                <i class="fe-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </li>

                        <li class="dropdown notification-list">
                            <a class="nav-link dropdown-toggle waves-effect" data-toggle="dropdown" href="~/Admin/#" role="button" aria-haspopup="false" aria-expanded="false">
                                <i class="fe-bell noti-icon"></i>
                                <span class="badge badge-danger rounded-circle noti-icon-badge">9</span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right dropdown-lg">

                                <!-- item-->
                                <div class="dropdown-item noti-title">
                                    <h5 class="m-0">
                                        <span class="float-right">
                                            <a href="~/Admin/" class="text-dark">
                                                <small>Clear All</small>
                                            </a>
                                        </span>Notification
                                    </h5>
                                </div>

                                <div class="slimscroll noti-scroll">

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item active">
                                        <div class="notify-icon">
                                            <img src="~/Admin/assets/images/users/user-1.jpg" class="img-fluid rounded-circle" alt="" />
                                        </div>
                                        <p class="notify-details">Cristina Pride</p>
                                        <p class="text-muted mb-0 user-msg">
                                            <small>Hi, How are you? What about our next meeting</small>
                                        </p>
                                    </a>

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                        <div class="notify-icon bg-primary">
                                            <i class="mdi mdi-comment-account-outline"></i>
                                        </div>
                                        <p class="notify-details">
                                            Caleb Flakelar commented on Admin
                                            <small class="text-muted">1 min ago</small>
                                        </p>
                                    </a>

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                        <div class="notify-icon">
                                            <img src="~/Admin/assets/images/users/user-4.jpg" class="img-fluid rounded-circle" alt="" />
                                        </div>
                                        <p class="notify-details">Karen Robinson</p>
                                        <p class="text-muted mb-0 user-msg">
                                            <small>Wow ! this admin looks good and awesome design</small>
                                        </p>
                                    </a>

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                        <div class="notify-icon bg-warning">
                                            <i class="mdi mdi-account-plus"></i>
                                        </div>
                                        <p class="notify-details">
                                            New user registered.
                                            <small class="text-muted">5 hours ago</small>
                                        </p>
                                    </a>

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                        <div class="notify-icon bg-info">
                                            <i class="mdi mdi-comment-account-outline"></i>
                                        </div>
                                        <p class="notify-details">
                                            Caleb Flakelar commented on Admin
                                            <small class="text-muted">4 days ago</small>
                                        </p>
                                    </a>

                                    <!-- item-->
                                    <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                        <div class="notify-icon bg-secondary">
                                            <i class="mdi mdi-heart"></i>
                                        </div>
                                        <p class="notify-details">
                                            Carlos Crouch liked
                                            <b>Admin</b>
                                            <small class="text-muted">13 days ago</small>
                                        </p>
                                    </a>
                                </div>

                                <!-- All-->
                                <a href="~/Admin/javascript:void(0);" class="dropdown-item text-center text-primary notify-item notify-all">
                                    View all
                                    <i class="fi-arrow-right"></i>
                                </a>

                            </div>
                        </li>

                        <li class="dropdown notification-list">
                            <a class="nav-link dropdown-toggle nav-user mr-0 waves-effect" data-toggle="dropdown" href="~/Admin/#" role="button" aria-haspopup="false" aria-expanded="false">
                                <span class="pro-user-name ml-1">
                                    @User.Identity.Name <i class="mdi mdi-chevron-down"></i>
                                </span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                                <!-- item-->
                                <div class="dropdown-header noti-title">
                                    <h6 class="text-overflow m-0">Welcome !</h6>
                                </div>

                                <!-- item-->
                                <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="fe-user"></i>
                                    <span>My Account</span>
                                </a>

                                <!-- item-->
                                <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="fe-settings"></i>
                                    <span>Settings</span>
                                </a>

                                <!-- item-->
                                <a href="~/Admin/javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="fe-lock"></i>
                                    <span>Lock Screen</span>
                                </a>

                                <div class="dropdown-divider"></div>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item" onclick="document.getElementById('logoutForm').submit();">
                                    <i class="fe-log-out"></i>
                                    <span>Logout</span>
                                </a>

                                <form id="logoutForm" asp-action="Logout" asp-controller="Accounts" method="post" style="display: none;">
                                </form>
                            </div>
                        </li>

                        <li class="dropdown notification-list">
                            <a href="~/Admin/javascript:void(0);" class="nav-link right-bar-toggle waves-effect">
                                <i class="fe-settings noti-icon"></i>
                            </a>
                        </li>

                    </ul>

                    <!-- LOGO -->
                    <div class="logo-box">
                        <a href="~/Admin/index.html" class="logo logo-light">
                            <span class="logo-lg">
                                @* <img src="~/Admin/assets/images/logo-light.png" alt="" height="16"> *@
                                <span class="courier-font">Hollowed Keys</span>
                            </span>
                            <span class="logo-sm">
                                @* <img src="~/Admin/assets/images/logo-sm.png" alt="" height="24"> *@
                                <span class="courier-font">HK</span>
                            </span>
                        </a>
                        <a href="~/Admin/index.html" class="logo logo-dark">
                            <span class="logo-lg">
                                <img src="~/Admin/assets/images/logo-dark.png" alt="" height="16">
                            </span>
                            <span class="logo-sm">
                                <img src="~/Admin/assets/images/logo-sm.png" alt="" height="24">
                            </span>
                        </a>
                    </div>

                    <div class="clearfix"></div>
                </div> <!-- end container-fluid-->
            </div>
            <!-- end Topbar -->

            <div class="topbar-menu">
                <div class="container-fluid">
                    <div id="navigation">
                        <!-- Navigation Menu-->
                        <ul class="navigation-menu">
                            <li>
                                <a asp-controller="Dashboards" asp-action="Index"><i class="fas fa-table mdi-invert-colors"></i> Dashboard</a>
                            </li>
                            <li>
                                <a asp-controller="Users" asp-action="Index"><i class="fas fa-user mdi-invert-colors"></i> Users</a>
                            </li>
                            <li>
                                <a asp-controller="Games" asp-action="Index"><i class="fas fa-gamepad mdi-invert-colors"></i> Games</a>
                            </li>
                            <li>
                                <a asp-controller="Genres" asp-action="Index"><i class="fas fa-bahai mdi-invert-colors"></i> Genres</a>
                            </li>
                            <li>
                                <a asp-controller="Publishers" asp-action="Index"><i class="fas fa-book-reader mdi-invert-colors"></i> Publishers</a>
                            </li>
                            <li>
                                <a asp-controller="TarotCards" asp-action="Index"><i class=" far fa-caret-square-up mdi-invert-colors"></i> Tarot Cards</a>
                            </li>

                        </ul>
                        <!-- End navigation menu -->

                        <div class="clearfix"></div>
                    </div>
                    <!-- end #navigation -->
                </div>
                <!-- end container -->
            </div>
            <!-- end navbar-custom -->

        </header>
        <!-- End Navigation Bar-->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">
                @RenderBody()
            </div> <!-- content -->
        </div>
        <!-- Footer Start -->
        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6">
                        2016 - 2020 &copy; Adminto theme by <a href="~/Admin/">Sheikh Abdul Wahab Ali</a>
                    </div>
                    <div class="col-md-6">
                        <div class="text-md-right footer-links d-none d-sm-block">
                            <a href="~/Admin/javascript:void(0);">About Us</a>
                            <a href="~/Admin/javascript:void(0);">Help</a>
                            <a href="~/Admin/javascript:void(0);">Contact Us</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- end Footer -->

    </div>

    <!-- ============================================================== -->
    <!-- End Page content -->
    <!-- ============================================================== -->
    <!-- END wrapper -->
    <!-- Vendor js -->
    <script src="~/Admin/assets/js/vendor.min.js"></script>

    <!-- knob plugin -->
    <script src="~/Admin/assets/libs/jquery-knob/jquery.knob.min.js"></script>

    <!--Morris Chart-->
    <script src="~/Admin/assets/libs/morris-js/morris.min.js"></script>
    <script src="~/Admin/assets/libs/raphael/raphael.min.js"></script>

    <!-- Dashboard init js-->
    <script src="~/Admin/assets/js/pages/dashboard.init.js"></script>

    <!-- App js -->
    <script src="~/Admin/assets/js/app.min.js"></script>
    @await RenderSectionAsync("ScriptForGameCreating", required:false)
    @await RenderSectionAsync("ScriptForGameUpdating", required:false)
    @await RenderSectionAsync("CreateTarotCardPageScripts", required: false)
    @await RenderSectionAsync("EditTarotCardPageScripts", required: false)
    @await RenderSectionAsync("DashboardScripts", required: false)
</body>
</html>